<span class="kw">import</span> os
<span class="kw">from</span> sikuli <span class="kw">import</span> *
<span class="kw">def</span> WebSignOn():
  <span class="kw">try</span>:
      openApp(<span class="str">"${BROWSER_EXECUTABLE}"</span>)
  <span class="kw">except</span>:
      <span class="kw">raise</span> FindFailed(<span class="str">"Browser not found"</span>)
  <span class="skw">wait</span>(<img src="VistALogin-1.png" />,<span class="dig">30</span>)
  <span class="skw">type</span>(Pattern(<img src="AccessCode-1.png" />).similar(<span class="dig">0.60</span>), <span class="str">'fakedoc1'</span>)
  <span class="skw">type</span>(Pattern(<img src="VAVerifyCode-1.png" />).similar(<span class="dig">0.40</span>).targetOffset(<span class="dig">30</span>,<span class="dig">0</span>), <span class="str">'1Doc!@#$\r'</span>)

<span class="kw">def</span> WebSignOff():
    <span class="skw">type</span>(Key.PAGE_DOWN)
    <span class="skw">wait</span>(<span class="dig">2</span>)
    links = [<img src="MainMenu-3.png" />,<img src="MainMenu.png" />]
    <span class="kw">for</span> value <span class="kw">in</span> range(<span class="dig">0</span>,<span class="dig">2</span>):
        <span class="kw">try</span>:
            <span class="skw">click</span>(links[value])
        <span class="kw">except</span>:
            <span class="kw">continue</span>
    <span class="skw">wait</span>(<img src="MainMenu-2.png" />,<span class="dig">5</span>)
    <span class="skw">click</span>(<img src="LogoutofVist-1.png" />)
    <span class="skw">wait</span>(<img src="LogoutofVis.png" />)
    <span class="skw">click</span>(<img src="OK-2.png" />)
    <span class="skw">wait</span>(<span class="dig">5</span>)
    sys.exit(<span class="dig">0</span>)

<span class="kw">def</span> ExitWindow():
    <span class="skw">type</span>(<span class="str">"f"</span>,KeyModifier.ALT)
